<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="32dp"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/textViewListItemZzimEmptyTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="찜한 칵테일이 없어요."
        style="@style/Font_SemiBold_22_Basic_White"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textViewListItemZzimEmptySubTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="마음에 드는 칵테일을 찜해보세요!"
        style="@style/Font_Medium_18_Basic_White"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@+id/textViewListItemZzimEmptyTitle"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>